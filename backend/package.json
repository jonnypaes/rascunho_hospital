{
  "name": "hospital-api",
  "version": "1.0.0",
  "description": "API para gerenciamento de agendamentos",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "prebuild": "npm install",
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "node --loader ts-node/esm --inspect=0.0.0.0:9229 src/server.ts",
    "dev2": "tsx watch src/server.ts",
    "dev3": "ts-node-dev --respawn --transpile-only --node-args \"--import ts-node/register/esm\" src/server.ts",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "mysql2": "^3.9.0",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
